{
  "manifest_version": 3,
  "name": "Happenstance",
  "description": "record web browsing location",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/happenstance-icon-16.png",
      "128": "images/happenstance-icon-128.png"
    }
  },
  "icons": {
    "16": "images/happenstance-icon-16.png",
    "128": "images/happenstance-icon-128.png"
  },
  "permissions": [
    "history"
  ]
}
